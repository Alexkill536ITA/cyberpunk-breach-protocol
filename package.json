{
  "name": "cyberpunk-hexcode-breach",
  "version": "1.0.1",
  "main": "app.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host=0.0.0.0",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "electron": "electron ./",
    "electron-build": "ng build && electron ./",
    "package-win-64": "ng build && electron-packager ./ cyberpunk-hexcode-breach --overwrite --asar=true --platform=win32 --arch=x64 --icon=src/assets/icons/win/icon.ico --prune=true --out=builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"cyberpunk-hexcode-breach\"",
    "package-win": "ng build && electron-packager ./ cyberpunk-hexcode-breach --overwrite --asar=true --platform=win32 --arch=ia32 --icon=src/assets/icons/win/icon.ico --prune=true --out=builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"cyberpunk-hexcode-breach\"",
    "package-linux": "ng build && electron-packager ./ cyberpunk-hexcode-breach --overwrite --asar=true --platform=linux --arch=x64 --icon=src/assets/icons/png/1024x1024.png --prune=true --out=builds",
    "package-mac": "ng build && electron-packager ./ --overwrite --platform=darwin --arch=x64 --icon=src/assets/icons/mac/icon.icns --prune=true --out=builds",
    "package-all": "ng build && npm run package-win && npm run package-win-64 && npm run package-linux && npm run package-mac"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.15",
    "@angular/common": "^20.3.15",
    "@angular/compiler": "^20.3.15",
    "@angular/core": "^20.3.15",
    "@angular/forms": "^20.3.15",
    "@angular/platform-browser": "^20.3.15",
    "@angular/platform-browser-dynamic": "^20.3.15",
    "@angular/router": "^20.3.15",
    "@ng-bootstrap/ng-bootstrap": "^19.0.1",
    "@popperjs/core": "^2.11.8",
    "@primeng/themes": "^20.3.0",
    "@types/node": "^20.9.2",
    "bootstrap": "^5.3.6",
    "bootstrap-icons": "^1.13.1",
    "ngx-electron": "^2.2.0",
    "primeicons": "^7.0.0",
    "primeng": "^20.3.0",
    "rxjs": "~7.8.0",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7",
    "tslib": "^2.8.1",
    "update-electron-app": "^3.1.1",
    "uuid": "^13.0.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular/build": "^20.3.12",
    "@angular/cli": "^20.3.12",
    "@angular/compiler-cli": "^20.3.15",
    "@types/jasmine": "~5.1.13",
    "electron": "^39.2.4",
    "jasmine-core": "~5.13.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.8.2"
  }
}
